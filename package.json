{
  "name": "ParseServer",
  "version": "1.4.0",
  "description": "An Parse API server using the parse-server module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParentPlanetApplication/ParseServer"
  },
  "license": "MIT",
  "dependencies": {
    "date.js": "^0.3.1",
    "express": "~4.11.x",
    "json-file-plus": "^3.3.0",
    "kerberos": "~0.0.x",
    "kue": "^0.11.1",
    "kue-scheduler": "^0.5.1",
    "mailgun-js": "^0.7.12",
    "parse": "^1.9.1",
    "parse-server": "^2.2.17",
    "parse-server-mandrill-adapter": "^1.1.1",
    "parse-server-simple-mailgun-adapter": "^1.0.0",
    "pm2": "^1.1.3"
  },
  "scripts": {
    "pm2:start": "pm2 start index.js --name $APP_NAME -l ParseServer_logs.log",
    "pm2:restart": "pm2 stop $APP_NAME && pm2 delete $APP_NAME && npm run pm2:start",
    "pm2": "if pm2 list | grep $APP_NAME; then npm run pm2:restart; else npm run pm2:start; fi",
    "start": "node index.js"
  },
  "engines": {
    "node": ">=4.3"
  }
}
